<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.multi_reminder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".shower"></activity>
        <activity
            android:name=".custom"
            android:icon="@drawable/ic_settings_blackdp"
            android:label="Custom reminder" />
        <activity
            android:name=".exercise"
            android:icon="@drawable/ic_workout"
            android:label="Workout" />
        <activity
            android:name=".cleaning"
            android:icon="@drawable/ic_clean"
            android:label="Clean" />
        <activity
            android:name=".hands_daily"
            android:icon="@drawable/ic_hand_wash"
            android:label="Wash hands" />
        <activity
            android:name=".eat_daily"
            android:icon="@drawable/ic_eat"
            android:label="Eat" />
        <activity
            android:name=".drink_daily"
            android:icon="@drawable/ic_water_glass"
            android:label="Drink Water" />
        <activity
            android:name=".MainActivity"
            android:label="Get your S#!T together">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>